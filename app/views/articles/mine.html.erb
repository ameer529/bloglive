<p id="notice"><%= notice %></p>

<h1>My Articles</h1>

<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Title</th>
      <th>Body</th>
      </tr>
  </thead>

  <tbody>
    <% @article.each do |article| %>
    <% #if article.user == current_user %>
      <tr>
        <td><%= link_to article.title , article %></td> 
        <td><%= article.body %></td>
      </tr>
      <% #end %>
    <% end %>
  </tbody>
</table>

</br>